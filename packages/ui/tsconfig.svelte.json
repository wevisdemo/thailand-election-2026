{
	"extends": "./tsconfig.json",
	"include": [".tmp/svelte/**.ts"],
	"compilerOptions": {
		"outDir": "dist/svelte",
		"module": "esnext",
		"moduleResolution": "node",
		"target": "esnext",
		"skipLibCheck": true,
		"declaration": true,
		"noImplicitAny": false,
		"removeComments": true,
		"noLib": false,
		"emitDecoratorMetadata": true,
		"sourceMap": true
	}
}
