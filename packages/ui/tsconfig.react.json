{
	"extends": "./tsconfig.json",
	"include": [".tmp/react/**.ts"],
	"compilerOptions": {
		"outDir": "dist/react",
		"target": "ES2020",
		"useDefineForClassFields": true,
		"lib": ["ES2020", "DOM", "DOM.Iterable"],
		"module": "ESNext",
		"skipLibCheck": true,
		"allowSyntheticDefaultImports": true,
		"declaration": true
	}
}
